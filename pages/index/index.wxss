.swiper_box {
    margin-left: 10px;
    margin-right: 10px;
    margin-bottom: 10px;
}


swiper-item image {
    width: 100%;
    height: 100%;
    display: inline-block;
    overflow: hidden;
}

view.text {
    display: flex;
    align-items: center;
    position: relative;
    padding: 10px 10px 10px 12px;
    font-size: 15px;
    color: #656565;
}

view.text:before {
    position: absolute;
    display: block;
    content: ' ';
    left: -5px;
    width: 2px;
    height: 100%;
    background-color: #09bb07;
}

/*立即认证  */

.quick-auth {
    padding-left: 2px;
    padding-right: 2px;
    margin-top: 10px;
    display: flex;
    font-size: 8px;
    color: white;
    background-color: #ec9c49;
    text-align: center;
    border-radius: 24px;
    border: 1px white solid;
}

/* 今日 */
.today-add-num {
    font-size: 11px;
    color: white;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 3px;
    padding-bottom: 3px;
    background-color: #ff4723;
    border-radius: 20px;
    position: relative;
    top: 24px;
    left: 45px;
}

.line_flag {
    height: 15px;
    padding: 0, 0, 0, 0;
    display: inline-block;
}

.line_flag_right {
    height: 15px;
    width: 20%;
    margin-left: 20px;
    padding: 0, 0, 0, 0;
    display: inline-block;
    text-align: center;
}

view.text text {
    margin-left: 10px;
    line-height: 18px;
    font-weight: bold;
}

.venues_box {
    padding: 0 10rpx;
}

.venues_item {
    display: inline-block;
    width: 49%;
    margin: 0 0.5%;
}

.venues_item image {
    width: 100%;
    height: 90px;
}

.choice_box {
    padding: 0 10rpx;
}

.choice_box .choice_list .choice_item {
    margin-bottom: 15px;
}

.choice_box .choice_list image {
    width: 100%;
    height: 200px;
}

.choice_box .choice_list .title {
    font-size: 12px;
    font-weight: bold;
    text-align: center;
    color: #333;
}

.center-item {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-left: 10px;
    margin-right: 10px;
    margin-bottom: 10px;
}

.center-text {
    font-size: 14px;
}
.cotainer-home{
  display: flex;
  height: 100%;
  flex-direction: column;
}
/*首页顶部UI*/

.home_top {
    display: flex;
    align-items: center;
    flex: 1;
    flex-direction: row;
    vertical-align: middle;
    justify-content: space-between;
    background-color: #f93;
    height: 340rpx;
    margin: 10px;
    border-radius: 6px 6px 6px 6px;
}
.center-view{
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex: 1;
}
.bottom-view{
  display: flex;
  flex: 1;
  flex-direction: row;
}

.home_top_text_template {
    left: 32px;
    color: white;
}

.home_top_text_my {
    left: 23px;
    color: white;
}

.home_my_p {
    display: flex;
    flex: 1;
    justify-content: center;
    flex-direction: column;
    margin-top: 40rpx;
    width: 80px;
    align-items: center;
    vertical-align: middle;
    height: 80px;
}

.home_template_p {
    width: 80px;
    height: 80px;
    display: flex;
    margin-top: 40rpx;
    flex: 1;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    vertical-align: middle;
}

.home_top_img_my {
    width: 56px;
    height: 48px;
}

.home_top_img_template {
    width: 56px;
    height: 48px;
}

.home_circle_p {
    display: flex;
    flex: 1;
    justify-content: center;
}

.ifu-p {
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    vertical-align: middle;
    width: 90px;
    height: 90px;
    background-color: transparent;
    text-align: center;
    border-radius: 100px;
    border: 3px white solid;
    margin-bottom: 20px;
}

/*医数值  */

.home_ifu_value {
    display: flex;
    font-size: 28px;
    text-align: center;
    color: white;
}

.home_my_value {
    font-size: 11px;
    text-align: center;
    margin-bottom: 5px;
    color: white;
}

.item_size {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    
}

.text {
    font-size: 12px;
}

.text_num {
    font-size: 12px;
    color: #f93;
}

.text_will_open {
    font-size: 12px;
    color: white;
}
